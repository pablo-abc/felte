{
  "name": "@felte/reporter-svelte",
  "version": "0.3.17",
  "description": "An error reporter for Felte using a Svelte component",
  "main": "dist/index.js",
  "svelte": "src/index.js",
  "browser": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "types/index.d.ts",
  "sideEffects": false,
  "author": "Pablo Berganza <pablo@berganza.dev>",
  "repository": "github:pablo-abc/felte",
  "homepage": "https://github.com/pablo-abc/felte/tree/main/packages/reporter-svelte",
  "keywords": [
    "svelte",
    "forms",
    "validation",
    "felte"
  ],
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build": "cross-env NODE_ENV=production rollup -c",
    "dev": "rollup -cw",
    "prepublishOnly": "pnpm build && pnpm test",
    "test": "jest",
    "test:ci": "jest --ci --coverage"
  },
  "license": "MIT",
  "dependencies": {
    "@felte/common": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "src",
    "types"
  ],
  "devDependencies": {
    "felte": "workspace:*",
    "rollup-plugin-svelte": "^7.1.0",
    "svelte": "^3.31.0",
    "svelte-preprocess": "^4.6.9"
  },
  "peerDependencies": {
    "svelte": "^3.31.0"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  }
}
