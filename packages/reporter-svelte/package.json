{
  "name": "@felte/reporter-svelte",
  "version": "1.2.0",
  "description": "An error reporter for Felte using a Svelte component",
  "main": "dist/index.cjs",
  "svelte": "src/index.js",
  "browser": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "types/index.d.ts",
  "type": "module",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "sideEffects": false,
  "author": {
    "name": "Pablo Berganza",
    "url": "https://pablo.berganza.dev"
  },
  "repository": "github:pablo-abc/felte",
  "homepage": "https://github.com/pablo-abc/felte/tree/main/packages/reporter-svelte",
  "keywords": [
    "svelte",
    "forms",
    "validation",
    "felte"
  ],
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build": "pnpm prebuild && cross-env NODE_ENV=production rollup -c",
    "dev": "rollup -cw",
    "prepublishOnly": "pnpm build && pnpm test",
    "vitest": "vitest",
    "test": "vitest run",
    "test:ci": "vitest run --coverage"
  },
  "license": "MIT",
  "dependencies": {
    "@felte/common": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "src",
    "types"
  ],
  "devDependencies": {
    "@testing-library/svelte": "^5.2.4",
    "felte": "workspace:*",
    "svelte": "^5.1.4",
    "vite": "^4.3.9"
  },
  "peerDependencies": {
    "svelte": "^3.31.0 || ^4.0.0 || ^5.0.0"
  },
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "svelte": "./src/index.js",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "default": "./dist/index.mjs"
    },
    "./package.json": "./package.json"
  }
}
