{
  "name": "@felte/solid",
  "version": "1.0.0-next.27",
  "description": "An extensible form library for SolidJS",
  "main": "dist/cjs/index.cjs",
  "browser": "dist/esm/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "type": "module",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "sideEffects": false,
  "author": "Pablo Berganza <pablo@berganza.dev>",
  "license": "MIT",
  "homepage": "https://felte.dev",
  "repository": "github:pablo-abc/felte",
  "funding": "https://www.buymeacoffee.com/pablo.abc",
  "keywords": [
    "solidjs",
    "solid",
    "forms",
    "validation"
  ],
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build": "pnpm prebuild && cross-env NODE_ENV=production rollup -c",
    "docs:build": "typedoc --out ../../docs",
    "dev": "rollup -cw",
    "prepublishOnly": "pnpm build && pnpm test",
    "test": "node --conditions browser node_modules/uvu/bin.js -r tsm -r global-jsdom/register tests -i common",
    "test:ci": "nyc -n src pnpm test"
  },
  "peerDependencies": {
    "solid-js": "^1.2.0"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@felte/core": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@felte/common": "workspace:*",
    "solid-js": "^1.2.0",
    "uvu": "^0.5.3"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs",
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  }
}
