{
  "name": "@felte/reporter-preact",
  "version": "1.1.10",
  "description": "An error reporter for Felte using a Preact component",
  "main": "dist/index.cjs",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "sideEffects": false,
  "author": {
    "name": "Pablo Berganza",
    "url": "https://pablo.berganza.dev"
  },
  "repository": "github:pablo-abc/felte",
  "homepage": "https://github.com/pablo-abc/felte/tree/main/packages/reporter-preact",
  "keywords": [
    "preactjs",
    "preact",
    "forms",
    "validation",
    "felte"
  ],
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "exports": {
    ".": {
      "types": "./dist/esm/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build": "pnpm prebuild && cross-env NODE_ENV=production rollup -c",
    "dev": "rollup -cw",
    "prepublishOnly": "pnpm run build",
    "vitest": "vitest",
    "test": "vitest run",
    "test:ci": "vitest run --coverage"
  },
  "dependencies": {
    "@felte/common": "workspace:*"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-typescript": "^7.18.6",
    "@felte/preact": "workspace:*",
    "@preact/preset-vite": "^2.1.5",
    "@testing-library/preact": "^3.2.2",
    "@testing-library/preact-hooks": "^1.1.0",
    "babel-jest": "^26.6.3",
    "preact": "^10.11.3",
    "tsc-watch": "^4.4.0",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "preact": "^10.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
