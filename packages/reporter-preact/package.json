{
  "name": "@felte/reporter-preact",
  "version": "1.0.3",
  "description": "An error reporter for Felte using a Preact component",
  "main": "dist/index.cjs",
  "module": "dist/esm/index.js",
  "sideEffects": false,
  "author": "Pablo Berganza <pablo@berganza.dev>",
  "repository": "github:pablo-abc/felte",
  "homepage": "https://github.com/pablo-abc/felte/tree/main/packages/reporter-preact",
  "keywords": [
    "preactjs",
    "preact",
    "forms",
    "validation",
    "felte"
  ],
  "license": "MIT",
  "types": "dist/types/index.d.ts",
  "type": "module",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build": "pnpm prebuild && rollup -c",
    "dev": "rollup -cw",
    "prepublishOnly": "pnpm run build",
    "test": "uvu -r tsm --tsmconfig tsm.cjs -r global-jsdom/register tests -i common",
    "test:ci": "nyc -n src pnpm test"
  },
  "dependencies": {
    "@felte/common": "workspace:*"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-typescript": "^7.14.5",
    "@felte/preact": "workspace:*",
    "@rollup/plugin-babel": "5.3.0",
    "@rollup/plugin-node-resolve": "13.0.5",
    "@testing-library/preact": "^2.0.1",
    "@testing-library/preact-hooks": "^1.1.0",
    "@types/node": "^15.12.4",
    "babel-jest": "^26.6.3",
    "preact": "^10.5.15",
    "rollup": "^2.52.1",
    "rollup-plugin-terser": "^7.0.2",
    "tsc-watch": "^4.4.0",
    "typescript": "~4.3.4"
  },
  "peerDependencies": {
    "preact": "^10.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
