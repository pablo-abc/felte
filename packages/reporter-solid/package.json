{
  "name": "@felte/reporter-solid",
  "version": "1.2.10",
  "description": "An error reporter for Felte using a Solid component",
  "main": "dist/cjs/index.js",
  "source": "src/index.tsx",
  "sideEffects": false,
  "author": {
    "name": "Pablo Berganza",
    "url": "https://pablo.berganza.dev"
  },
  "repository": "github:pablo-abc/felte",
  "homepage": "https://github.com/pablo-abc/felte/tree/main/packages/reporter-solid",
  "keywords": [
    "solidjs",
    "solid",
    "forms",
    "validation",
    "felte"
  ],
  "license": "MIT",
  "type": "module",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "solid": "./dist/source/index.jsx",
      "import": "./dist/esm/index.js",
      "browser": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "node": "./dist/cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build": "pnpm prebuild && NODE_ENV=production rollup -c",
    "dev": "rollup -cw",
    "prepublishOnly": "pnpm run build",
    "test": "jest",
    "test:ci": "jest --coverage"
  },
  "dependencies": {
    "@felte/common": "workspace:*"
  },
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@felte/solid": "workspace:*",
    "@solidjs/testing-library": "^0.8.4",
    "@testing-library/jest-dom": "^5.16.5",
    "babel-jest": "^26.6.3",
    "jest": "^29.6.4",
    "jest-environment-jsdom": "^29.6.4",
    "rollup-preset-solid": "^2.0.1",
    "solid-jest": "^0.2.0",
    "solid-js": "^1.8.6",
    "typescript": "~4.9.3"
  },
  "peerDependencies": {
    "solid-js": "^1.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
